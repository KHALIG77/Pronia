 @model HomeViewModel 
 
@{
    bool status = true;
}

<!-- Begin Slider Area -->
<div class="slider-area">
    <!-- Main Slider -->
    <div class="swiper-container main-slider swiper-arrow with-bg_white">
        <div class="swiper-wrapper">
           @foreach (var slide in Model.Slider)
            {
                <div class="swiper-slide animation-style-01">
                    <div class="slide-inner style-1 bg-height"
                         data-bg-image="/uploads/slider-bg/@slide.BgUrl">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-6 order-2 order-lg-1 align-self-center">
                                    <div class="slide-content text-black">
                                        <span class="offer">@slide.Offer</span>
                                        <h2 class="title">@slide.Title</h2>
                                        <p class="short-desc">
                                           @slide.Description
                                        </p>
                                        <div class="btn-wrap">
                                            <a class="btn btn-custom-size xl-size btn-pronia-primary"
                                               href="@slide.ButtonUrl">@slide.ButtonText</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-8 offset-md-2 offset-lg-0 order-1 order-lg-2">
                                    <div class="inner-img">
                                        <div class="scene fill">
                                            <div class="expand-width" data-depth="0.2">
                                                <img src="~/uploads/slider-inner/@slide.ImageUrl"
                                                     alt="Inner Image" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
           
        </div>
        <!-- Add Pagination -->
        <div class="swiper-pagination d-md-none"></div>

        <!-- Add Arrows -->
        <div class="swiper-button-prev"></div>
        <div class="swiper-button-next"></div>
    </div>
</div>
<!-- Slider Area End Here -->
<!-- Begin Shipping Area -->
<div class="shipping-area section-space-top-100">
    <div class="container">
        <div class="shipping-bg">
            <div class="row shipping-wrap">
                @for (int i = 0; i < Model.Feature.Count; i++)
                {
                    <div class="col-lg-4 col-md-6 mt-4">
                        <div class="shipping-item">
                            <div class="shipping-img">
                                <img src="~/uploads/features/@Model.Feature[i].IconUrl"
                                     alt="Shipping Icon" />
                            </div>
                            <div class="shipping-content">
                                <h2 class="title">@Model.Feature[i].Title</h2>
                                <p class="short-desc mb-0">@Model.Feature[i].Description</p>
                            </div>
                        </div>
                    </div>
                }
               
             
        </div>
    </div>
    </div>
</div>
<!-- Shipping Area End Here -->
