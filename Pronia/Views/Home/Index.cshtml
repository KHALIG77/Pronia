<!-- Begin Banner Area -->
<div class="banner-area section-space-top-90">
    <div class="container">
        <div class="row g-min-30 g-4">
            
           @for (int i = 0; i<Model.Banner.Count ; i++)
            {
                if (i % 3 != 0)
                {
                    status = false;
                }
                if (i % 4 == 0)
                {
                    status = true;
                }

                if (status)
                {
                    <div class="col-lg-8">
                        <div class="banner-item img-hover-effect">
                            <div class="banner-img">
                                <img src="~/uploads/banners/@Model.Banner[i].ImageUrl"
                                     alt="Banner Image" />
                            </div>
                            <div class="banner-content text-position-left">
                                <span class="collection">@Model.Banner[i].Title</span>
                                <h3 class="title">
                                   @Model.Banner[i].Description
                                </h3>
                                <div class="button-wrap">
                                    <a class="btn btn-custom-size btn-pronia-primary"
                                       href="@Model.Banner[i].ButtonUrl">@Model.Banner[i].ButtonText</a>
                                </div>
                            </div>
                        </div>
                    </div>

                    status = false;


                }
                else
                {
                    <div class="col-lg-4 col-md-6">
                        <div class="banner-item img-hover-effect">
                            <div class="banner-img">
                                <img src="~/uploads/banners/@Model.Banner[i].ImageUrl"
                                     alt="Banner Image" />
                            </div>
                            <div class="banner-content text-position-center">
                                <span class="collection">@Model.Banner[i].Title</span>
                                <h3 class="title">@Model.Banner[i].Description</h3>
                                <div class="button-wrap">
                                    <a class="btn btn-custom-size lg-size btn-pronia-primary"
                                       href="@Model.Banner[i].ButtonUrl">@Model.Banner[i].ButtonText</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    status = true;
                }
                
            }
           
           
            
            </div>
        </div>
    </div>
</div>
